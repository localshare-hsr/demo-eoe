<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" fx:id="gridPane" maxHeight="-Infinity" minHeight="-Infinity"
          minWidth="-Infinity" xmlns="http://javafx.com/javafx/8">
    <columnConstraints>
        <ColumnConstraints halignment="LEFT" hgrow="ALWAYS" percentWidth="20.0"/>
        <ColumnConstraints hgrow="ALWAYS" percentWidth="35.0"/>
        <ColumnConstraints halignment="LEFT" hgrow="ALWAYS" percentWidth="10.0"/>
        <ColumnConstraints halignment="LEFT" hgrow="ALWAYS" percentWidth="35.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <Label fx:id="ip" text="IP:" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <padding>
            <Insets left="10.0" right="10.0"/>
        </padding>
    </Label>
    <Label fx:id="fn" prefHeight="17.0" prefWidth="72.0" text="FN:" GridPane.columnIndex="3" GridPane.rowIndex="1">
        <padding>
            <Insets left="10.0" right="10.0"/>
        </padding>
    </Label>
    <Label fx:id="finger" prefHeight="17.0" prefWidth="125.0" text="Fingerprint:" GridPane.columnIndex="1"
           GridPane.rowIndex="2">
        <padding>
            <Insets left="10.0" right="10.0"/>
        </padding>
    </Label>
    <Label fx:id="dn" prefHeight="17.0" prefWidth="91.0" text="DN:" GridPane.columnIndex="3" GridPane.rowIndex="2">
        <padding>
            <Insets left="10.0" right="10.0"/>
        </padding>
    </Label>
    <Label text="IP:" GridPane.rowIndex="1"/>
    <Label text="Fingerprint:" GridPane.rowIndex="2"/>
    <Label text="FN:" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
    <Label text="DN:" GridPane.columnIndex="2" GridPane.rowIndex="2"/>
</GridPane>
