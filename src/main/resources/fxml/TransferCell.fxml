<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane fx:id="paneTransfer" xmlns="http://javafx.com/javafx/8.0.171"
  xmlns:fx="http://javafx.com/fxml/1">
  <HBox alignment="CENTER_LEFT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
    <AnchorPane>
      <HBox.margin>
        <Insets/>
      </HBox.margin>
      <ImageView fx:id="finishedIcon" layoutY="7.0" visible="false">
        <Image url="@../success.png"/>
      </ImageView>
      <ImageView fx:id="downloadingIcon" layoutY="4.0">
        <Image url="@../download.png"/>
      </ImageView>
    </AnchorPane>
    <VBox alignment="CENTER" HBox.hgrow="ALWAYS">
      <HBox.margin>
        <Insets left="5.0"/>
      </HBox.margin>
      <Label fx:id="filename" maxWidth="1.7976931348623157E308" text="filename"/>
      <ProgressBar fx:id="transferProgressBar" maxHeight="10.0" maxWidth="1.7976931348623157E308"
        minHeight="10.0" prefHeight="10.0" progress="0.0" visible="false" VBox.vgrow="ALWAYS"/>
      <HBox fx:id="progressHbox" visible="false">
        <Label fx:id="secondsToGo" text="seconds"/>
        <Label text="left -">
          <HBox.margin>
            <Insets left="5.0" right="5.0"/>
          </HBox.margin>
        </Label>
        <Label fx:id="sizeCurrent" text="size"/>
        <Label text="of">
          <HBox.margin>
            <Insets left="5.0" right="5.0"/>
          </HBox.margin>
        </Label>
        <Label fx:id="sizeTotal" text="size"/>
        <Label fx:id="size1" text="(">
          <padding>
            <Insets left="5.0"/>
          </padding>
        </Label>
        <Label fx:id="transferSpeed" text="transferSpeed"/>
        <Label text=")"/>
      </HBox>
    </VBox>
    <ImageView fx:id="buttonAccept" accessibleRole="BUTTON">
      <HBox.margin>
        <Insets right="10.0"/>
      </HBox.margin>
      <Image url="@../play.png"/>
    </ImageView>
    <ImageView fx:id="buttonCancelTransfer" accessibleRole="BUTTON">
      <HBox.margin>
        <Insets right="5.0"/>
      </HBox.margin>
      <Image url="@../cancel.png"/>
    </ImageView>
  </HBox>
</AnchorPane>
