<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<TabPane xmlns:fx="http://javafx.com/fxml/1" onDragExited="#onWindowDragExit"
  onDragOver="#onWindowDragEnter" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8"
  fx:controller="ch.hsr.epj.localshare.demo.gui.application.MainWindowController">
  <tabs>
    <Tab text="Transfers">
      <content>
        <AnchorPane>
          <children>
            <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
              </padding>
              <children>
                <GridPane VBox.vgrow="ALWAYS">
                  <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="206.0" minWidth="10.0"
                      prefWidth="127.0"/>
                    <ColumnConstraints hgrow="SOMETIMES"/>
                  </columnConstraints>
                  <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0"/>
                  </rowConstraints>
                  <padding>
                    <Insets bottom="20.0"/>
                  </padding>
                  <children>
                    <Label text="Friendly Name:"/>
                    <Label GridPane.rowIndex="1" text="IP-Address:"/>
                    <Label GridPane.rowIndex="2" text="Fingerprint:"/>
                    <Text GridPane.columnIndex="1" GridPane.rowIndex="1" strokeType="OUTSIDE"
                      strokeWidth="0.0" text="172.96.12.88" wrappingWidth="160.7392578125"
                      fx:id="ipAddressText"/>
                    <Text GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2"
                      GridPane.vgrow="ALWAYS" strokeType="OUTSIDE" strokeWidth="0.0" text="1234"
                      wrappingWidth="581.7392578125" fx:id="fingerPrintText"/>
                    <Text GridPane.columnIndex="1" strokeType="OUTSIDE" strokeWidth="0.0"
                      text="hansWurst" wrappingWidth="116.7392578125" fx:id="friendlyNameText"/>
                  </children>
                </GridPane>
                <HBox VBox.vgrow="ALWAYS">
                  <children>
                    <VBox HBox.hgrow="ALWAYS" id="testRefresh">
                      <padding>
                        <Insets right="10.0"/>
                      </padding>
                      <children>
                        <Text VBox.vgrow="ALWAYS" strokeType="OUTSIDE" strokeWidth="0.0"
                          text="Available Peers"/>
                        <ListView VBox.vgrow="ALWAYS" onMouseClicked="#onListItemDoubleClick"
                          fx:id="listView"/>
                      </children>
                    </VBox>
                    <VBox HBox.hgrow="ALWAYS">
                      <children>
                        <Text VBox.vgrow="ALWAYS" strokeType="OUTSIDE" strokeWidth="0.0"
                          text="Available Downloads"/>
                        <ListView VBox.vgrow="ALWAYS" onMouseClicked="#onListItemDoubleClick"
                          fx:id="listViewTransfer"/>
                      </children>
                    </VBox>
                  </children>
                </HBox>
                <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                  onAction="#refreshList" text="Refresh">
                  <VBox.margin>
                    <Insets top="10.0"/>
                  </VBox.margin>
                </Button>
              </children>
            </VBox>
          </children>
        </AnchorPane>
      </content>
    </Tab>
    <Tab text="Channels">
      <content>
        <fx:include source="ChannelView.fxml"/>
      </content>
    </Tab>
    <Tab text="Settings">
      <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
          <children>
            <SplitPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"
              dividerPositions="0.39266304347826086" layoutX="13.0" layoutY="13.0">
              <items>
                <AnchorPane maxWidth="-Infinity" minHeight="0.0" minWidth="-Infinity"
                  prefHeight="589.0" prefWidth="280.0">
                  <children>
                    <Button id="button_prefernces" layoutX="31.0" layoutY="46.0"
                      mnemonicParsing="false" onAction="#handlePreferencesButtonAction"
                      prefHeight="43.0" prefWidth="217.0" text="Preferences"/>
                    <Button layoutX="31.0" layoutY="117.0" mnemonicParsing="false" prefHeight="43.0"
                      prefWidth="217.0" text="Button"/>
                  </children>
                </AnchorPane>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="561.0" prefWidth="415.0"
                  fx:id="preferencesRootPane"/>
              </items>
            </SplitPane>
          </children>
        </AnchorPane>
      </content>
    </Tab>
  </tabs>
</TabPane>
